      "type": "file2",
      "folder": "Images",
      "filter": "^.*\\\\.(jpg|JPG|gif|GIF|png|PNG)$"
    },
    "FirstLevelImageX": {
      "type": "imagex",
      "uploadhidden": false,
      "fileExtensions": "gif|jpg|jpeg|tiff|png",
      "fileMaxSize": 2000000,
      "showOverwrite": true,
      "overwrite": false,
      "showCropper": false
    },
